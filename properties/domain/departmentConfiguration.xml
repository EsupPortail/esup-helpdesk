<?xml version="1.0" encoding="UTF-8" ?>

<beans 
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop.xsd"
	>

	<bean 
		id="departmentConfigurator"
		class="org.esupportail.helpdesk.domain.departmentConfiguration.DepartmentConfiguratorImpl"
		lazy-init="true"
		>
		<property name="enabled" value="${departmentConfigurator.enabled}" />
		<property name="spentTimeNeeded" value="${departmentConfigurator.spentTimeNeeded}" /> 
	</bean>

</beans>
